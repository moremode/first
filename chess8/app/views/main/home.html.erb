<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
  <script type="text/javascript">
      function externalLinks() {
          links = document.getElementsByTagName("a");
          for (i=0; i<links.length; i++) {
              link = links[i];
              if (link.getAttribute("href") && link.getAttribute("rel") == "external")
                  link.target = "_blank";
          }
      }
      window.onload = externalLinks;
  </script>
</head>
<body>
  <div class="all">
    <div style="height: 1px"></div>
    <h1 class="header1">Champions</h1>
    <div class="wins-container">
      <a href="<%= @wiki[@rands[0]] %>" rel="external" class="div-href">
        <div class="winner">
          <img src="<%= image_url((@rands[0] + 1).to_s + '.jpg') %>" alt="">
          <p><%= @el_names[@rands[0]] %></p>
          <p><%= @years_life[@rands[0]] %></p>
          <p><%= @years_wins[@rands[0]] %></p>
        </div>
      </a>
      <a href="<%= @wiki[@rands[1]] %>" rel="external" class="div-href">
        <div class="winner">
          <img src="<%= image_url((@rands[1] + 1).to_s + '.jpg') %>" alt="">
          <p><%= @el_names[@rands[1]] %></p>
          <p><%= @years_life[@rands[1]] %></p>
          <p><%= @years_wins[@rands[1]] %></p>
        </div>
      </a>
      <a href="<%= @wiki[@rands[2]] %>" rel="external" class="div-href">
        <div class="winner">
          <img src="<%= image_url((@rands[2] + 1).to_s + '.jpg') %>" alt="">
          <p><%= @el_names[@rands[2]] %></p>
          <p><%= @years_life[@rands[2]] %></p>
          <p><%= @years_wins[@rands[2]] %></p>
        </div>
      </a>
    </div>
  </div>
</body>
</html>